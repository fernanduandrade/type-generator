{
  "name": "fetch-type-provider",
  "version": "1.0.7",
  "description": "A package that fetches a json and return the properties with it types",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "ts-node ./src/app.ts",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fernanduandrade/type-generator.git"
  },
  "keywords": [
    "fetch",
    "type generator",
    "fetch json",
    "type provider"
  ],
  "author": "Fernando Andrade <onanduandrade@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fernanduandrade/type-generator/issues"
  },
  "homepage": "https://github.com/fernanduandrade/type-generator#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/copy-paste": "^1.1.30",
    "@types/jest": "^29.2.0",
    "@types/prompt-sync": "^4.2.0",
    "@types/ramda": "^0.28.17",
    "jest": "^29.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^1.1.3",
    "copy-paste": "^1.3.0",
    "prompt-sync": "^4.2.0",
    "ramda": "^0.28.0"
  },
  "bin": {
    "fetch-type": "./index.js"
  }
}
